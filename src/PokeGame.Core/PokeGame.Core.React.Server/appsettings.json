{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceInfo": {
    "ReleaseName": "PokeGame.Core.React.Server",
    "ReleaseVersion": "1.0.0"
  },
  "PokeGameBackendSettings": {
    "PokeGameApiProdUri": "https://localhost:7244",
    "PokeGameSignalRProdUri": "https://localhost:7017"
  },
  "HstsOptions": {
    "IncludeSubDomains": true,
    "Preload": true,
    "MaxAgeInSeconds": "31536000"
  },
  "RequestTimeout": 30
}
